<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <FrameLayout
        android:layout_width="442dp"
        android:layout_height="65dp"
        android:background="#fff"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="-22dp">

    </FrameLayout>

    <FrameLayout
        android:id="@+id/backg"
        android:layout_width="442dp"
        android:layout_height="690dp"
        android:background="#17171c"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="46dp">

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="426dp"
        android:layout_height="692dp"
        android:background="#fff"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="46dp">

    </FrameLayout>

    <View
        android:id="@+id/divider2"
        android:layout_width="419dp"
        android:layout_height="2dp"
        android:background="#e4e5e5"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="44dp" />

    <View
        android:id="@+id/divider3"
        android:layout_width="400dp"
        android:layout_height="1dp"
        android:layout_marginStart="16dp"
        android:background="#e4e5e5"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="97dp" />

    <View
        android:id="@+id/divider5"
        android:layout_width="400dp"
        android:layout_height="1dp"
        android:layout_marginStart="16dp"
        android:background="#e4e5e5"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="152dp" />

    <View
        android:id="@+id/divider6"
        android:layout_width="400dp"
        android:layout_height="1dp"
        android:layout_marginStart="16dp"
        android:background="#e4e5e5"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="209dp" />

    <View
        android:id="@+id/divider7"
        android:layout_width="400dp"
        android:layout_height="1dp"
        android:layout_marginStart="16dp"
        android:background="#e4e5e5"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="636dp" />

    <CheckedTextView
        android:id="@+id/checkedTextView"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginTop="16dp"
        android:fontFamily="sans-serif-medium"
        android:text="Правила игры"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.952"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableRow" />

    <CheckedTextView
        android:id="@+id/checkedTextView8"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginTop="72dp"
        android:fontFamily="sans-serif-medium"
        android:text="Исключить участника"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.943"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableRow" />

    <CheckedTextView
        android:id="@+id/checkedTextView9"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginTop="128dp"
        android:fontFamily="sans-serif-medium"
        android:text="Игровой магазин"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.949"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableRow" />

    <CheckedTextView
        android:id="@+id/checkedTextView10"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:text="Официальный сайт"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.105"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="653dp" />

    <CheckedTextView
        android:id="@+id/checkedTextView11"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:fontFamily="sans-serif-black"
        android:text="wwg.company"
        android:textColor="#537ede"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.094"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="669dp" />

    <CheckedTextView
        android:id="@+id/checkedTextView12"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:fontFamily="sans-serif-light"
        android:text="(c) 2019"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.081"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="693dp" />

    <TableRow
        android:id="@+id/tableRow"
        android:layout_width="380dp"
        android:layout_height="45dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="115dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="0dp"
            android:contentDescription="TODO"
            android:padding="0dp"
            app:srcCompat="@drawable/wolo" />

        <ImageButton
            android:id="@+id/topmenu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/menuactive"
            android:contentDescription="TODO"
            app:srcCompat="@drawable/menuactive" />
    </TableRow>

    <TableLayout
        android:layout_width="380dp"
        android:layout_height="645dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="64dp">

    </TableLayout>

    <ImageButton
        android:id="@+id/info"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="12dp"
        android:padding="0px"
        android:scaleType="fitCenter"
        android:src="@drawable/info"
        android:background="#000"
        app:layout_constraintTop_toBottomOf="@+id/divider2"
        tools:layout_editor_absoluteX="31dp" />

    <ImageButton
        android:id="@+id/DeleteUser"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="68dp"
        android:background="#000"
        android:padding="0px"
        android:scaleType="fitCenter"
        android:src="@drawable/deleteuser"
        app:layout_constraintTop_toBottomOf="@+id/divider2"
        tools:layout_editor_absoluteX="31dp" />

    <ImageButton
        android:id="@+id/shop"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="124dp"
        android:background="#000"
        android:padding="0px"
        android:scaleType="fitCenter"
        android:src="@drawable/shop"
        app:layout_constraintTop_toBottomOf="@+id/divider2"
        tools:layout_editor_absoluteX="31dp" />

    <ImageButton
        android:id="@+id/wwglogo"
        android:layout_width="120dp"
        android:layout_height="70dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:background="#000"
        android:padding="0px"
        android:scaleType="fitCenter"
        android:src="@drawable/wwg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</android.support.constraint.ConstraintLayout>